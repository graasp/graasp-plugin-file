{
  "name": "graasp-plugin-file",
  "version": "0.1.0",
  "description": "File fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build && yarn hooks:install",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest  --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graasp/graasp-plugin-file.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-file/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-file#readme",
  "dependencies": {
    "aws-sdk": "^2.1051.0",
    "content-disposition": "0.5.4",
    "fastify-multipart": "5.2.1",
    "http-status-codes": "2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/content-disposition": "0.5.4",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "fastify": "3.25.3",
    "graasp-test": "github:graasp/graasp-test",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
