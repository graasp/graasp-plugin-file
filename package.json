{
  "name": "graasp-plugin-file",
  "version": "0.1.0",
  "description": "File fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build && yarn hooks:install",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graasp/graasp-plugin-file.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-file/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-file#readme",
  "dependencies": {
    "@fastify/multipart": "6.0.0",
    "@graasp/translations": "github:graasp/graasp-translations",
    "aws-sdk": "2.1111.0",
    "content-disposition": "0.5.4",
    "fs-extra": "10.1.0",
    "http-status-codes": "2.2.0",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@graasp/sdk": "github:graasp/graasp-utils#15/testUtils",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/content-disposition": "0.5.4",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "fastify": "3.29.1",
    "graasp-test": "github:graasp/graasp-test#23/sdk",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.1",
  "resolutions": {
